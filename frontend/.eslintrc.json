{
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/react-in-jsx-scope": "off",
    "react/jsx-one-expression-per-line": "off",
    "import/no-extraneous-dependencies": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "object-curly-newline": "off"
  },
  "env": {
    "jest": true,
    "browser": true
  }
}
